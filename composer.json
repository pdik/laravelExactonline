{
  "name": "pdik/laravelexactonline",
  "description": "Laravel exactonline package",
  "homepage": "https://github.com/pdik/laravelexactonline",
  "license": "MIT",
  "authors": [
    {
      "name": "pepijn dik",
      "email": "pepijn@pdik.nl",
      "homepage": "https://pdik.nl",
      "role": "Developer"
    },
    {
      "name": "Pascal Lieverse",
      "email": "p.lieverse@brightness-group.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4",
    "picqer/exact-php-client": "^3.31",
    "illuminate/routing": "^8.0",
    "illuminate/support": "^8.0",
    "illuminate/view": "^8.0",
    "illuminate/console": "^8.0",
    "illuminate/database": "^8.0",
    "illuminate/queue": "^8.0",
    "illuminate/events": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.10"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Pdik\\LaravelExactOnline\\": "src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pdik\\LaravelExactOnline\\LaravelExactOnlineServiceProvider"
      ],
      "aliases": {
        "LaravelExactOnline": "Pdik\\LaravelExactOnline\\LaravelExactOnlineFacade"
      }
    }
  }
}